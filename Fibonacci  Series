import java.util.*;
public class FibonacciSeries {
    public static void main(String[] args){
        int n1=0,n2=1;
        Scanner sc=new Scanner(System.in); 
        System.out.println("enter the count for fibonacci numbers:"); //enter the count to print fibonacci series
        int count=sc.nextInt(); //takes input from keyboard
        System.out.printf(n1+"\t"+n2+"\t"); //prints 0 and 1
        for(int i=2;i<count;i++){
            int n3=n1+n2;
            System.out.printf("%d\t",n3); //prints next numbers
            n1=n2;
            n2=n3;
            sc.close(); //closes scanner
        }

    }
    
}
