import java.util.*;
class Equal
{
    
    void EqualNumber(int a, int b){      //instance method
       
        if(a==b){
            System.out.printf("%d and %d are equal",a,b);
        }
        
        
        else 
        {
            System.out.printf("%d and %d are not equal",a,b);
            
        }

    }

}
public class TwoEqualNumbers {
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the first number:\t");
        int a=sc.nextInt();      //read first number
        System.out.println("enter the second number:\t");
        int b=sc.nextInt();     //read second number
        Equal s=new Equal(); //object creatiion
        s.EqualNumber(a,b); //instance method calling
        sc.close();                             

    }
    
}
